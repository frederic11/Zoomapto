{
  "expo": {
    "name": "Zoomapto",
    "slug": "Zoomapto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CC0E00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CC0E00"
      },
      "package": "com.frederictech.zoomapto",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB_0SUcP-QTmq0Tv5giXwQJRipkKDo3Oco"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
